<script>
var mySwiper3 = new Swiper('.suites-slider', {
  // Optional parameter
  pagination: '.pag-suites',
  paginationClickable: true,
  nextButton: '.swiper-button-next',
  prevButton: '.swiper-button-prev',
  freeMode: true,
  freeModeMomentumRatio: .75,
  freeModeMomentumVelocityRatio: 2,
  speed: 500,
   // Default parameters
  slidesPerView: 3.25,
  spaceBetween: 16,
  // Responsive breakpoints
  breakpoints: {
    // when window width is <= 320px
    320: {
      slidesPerView: 1.75,
      spaceBetween: 12
    },
    // when window width is <= 480px
    480: {
      slidesPerView: 1.5,
      spaceBetween: 12
    },
    // when window width is <= 640px
    640: {
      slidesPerView: 1.75,
      spaceBetween: 12
    },
    // when window width is <= 768px
    768: {
      slidesPerView: 2.25,
      spaceBetween: 12
    },
     // when window width is <= 1440px
    1440: {
      slidesPerView: 3.25,
      spaceBetween: 12
    }
  }

  
});
  
</script>

